<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ElectroSEnergy_ES">

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:text="Electrostatic Energy"
        android:textSize="36sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButton11" />

    <Button
        android:id="@+id/calculate2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="96dp"
        android:onClick="calcEE"
        android:text="Calculate"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/distance_ee" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="24dp"
        android:layout_marginRight="24dp"
        android:text="Distance"
        android:textColor="#E3526E"
        android:textSize="18sp"
        app:layout_constraintEnd_toStartOf="@+id/distance_ee"
        app:layout_constraintTop_toBottomOf="@+id/textView17" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="24dp"
        android:layout_marginRight="24dp"
        android:text="Electrostats Energy"
        android:textColor="#E3526E"
        android:textSize="18sp"
        app:layout_constraintEnd_toStartOf="@+id/esEnergy"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <EditText
        android:id="@+id/distance_ee"
        android:layout_width="96dp"
        android:layout_height="45dp"
        android:layout_marginEnd="90dp"
        android:layout_marginRight="90dp"
        android:autoText="false"
        android:background="#E3526E"
        android:ems="10"
        android:freezesText="false"
        android:inputType="text"
        android:textSize="18sp"
        app:flow_horizontalAlign="center"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView8"
        app:layout_constraintEnd_toEndOf="parent" />

    <EditText
        android:id="@+id/esEnergy"
        android:layout_width="96dp"
        android:layout_height="45dp"
        android:layout_marginEnd="90dp"
        android:layout_marginRight="90dp"
        android:autoText="false"
        android:background="#E3526E"
        android:ems="10"
        android:freezesText="false"
        android:inputType="text"
        android:textSize="18sp"
        app:flow_horizontalAlign="center"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView13"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="(Electrostatics)"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

    <TextView
        android:id="@+id/result_ee"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:text="RESULT"
        android:textSize="30sp"
        android:textStyle="bold|italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/calculate2" />

    <EditText
        android:id="@+id/charge_ee"
        android:layout_width="96dp"
        android:layout_height="45dp"
        android:layout_marginEnd="90dp"
        android:layout_marginRight="90dp"
        android:autoText="false"
        android:background="#E3526E"
        android:ems="10"
        android:freezesText="false"
        android:inputType="text"
        android:textSize="18sp"
        app:flow_horizontalAlign="center"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView12"
        app:layout_constraintEnd_toEndOf="parent" />

    <EditText
        android:id="@+id/charge_ee2"
        android:layout_width="96dp"
        android:layout_height="45dp"
        android:layout_marginEnd="90dp"
        android:layout_marginRight="90dp"
        android:autoText="false"
        android:background="#E3526E"
        android:ems="10"
        android:freezesText="false"
        android:inputType="text"
        android:textSize="18sp"
        app:flow_horizontalAlign="center"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView17"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="24dp"
        android:layout_marginRight="24dp"
        android:text="First Charge"
        android:textColor="#E3526E"
        android:textSize="18sp"
        app:layout_constraintEnd_toStartOf="@+id/charge_ee"
        app:layout_constraintTop_toBottomOf="@+id/textView14" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="24dp"
        android:layout_marginRight="24dp"
        android:text="Second Charge"
        android:textColor="#E3526E"
        android:textSize="18sp"
        app:layout_constraintEnd_toStartOf="@+id/charge_ee2"
        app:layout_constraintTop_toBottomOf="@+id/textView12" />

    <ImageButton
        android:id="@+id/imageButton11"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginTop="28dp"
        android:background="#00FFFFFF"
        android:onClick="homeIntent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo" />

</androidx.constraintlayout.widget.ConstraintLayout>